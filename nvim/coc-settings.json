{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "eslint.autoFixOnSave": true,
  "eslint.workingDirectories": [
    "./packages/core",
    "./packages/documentation",
    "./packages/playground"
  ],
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "markdown.mdx"
  ],
  "diagnostic.warningSign": "⚠",
  "diagnostic.errorSign": "✖",
  "diagnostic.infoSign": "⩒",
  "diagnostic.hintSign": "⊹"
}
